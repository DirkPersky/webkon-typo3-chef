{
	"name": "dirkpersky/webkon-typo3-chef",
	"description": "TYPO3 CMS Base Distribution - web kon",
	"type": "project",
	"repositories": [
		{ "type": "composer", "url": "https://composer.typo3.org/" }
	],
	"authors": [
		{
			"name": "Dirk Persky",
			"role": "Webdeveloper",
			"homepage": "http://dp-dvelop.de"
		}
	],
	"license": "GPL-2.0+",
	"require": {
		"typo3/cms-backend": "^9.5",
		"typo3/cms-core": "^9.5",
		"typo3/cms-extbase": "^9.5",
		"typo3/cms-extensionmanager": "^9.5",
		"typo3/cms-filelist": "^9.5",
		"typo3/cms-fluid": "^9.5",
		"typo3/cms-frontend": "^9.5",
		"typo3/cms-install": "^9.5",
		"typo3/cms-recordlist": "^9.5",
		"typo3/cms-beuser":"^9.5",
		"typo3/cms-rte-ckeditor": "^9.5",
		"typo3/cms-indexed-search": "^9.5",
		"typo3/cms-fluid-styled-content": "^9.5",
		"typo3/cms-filemetadata":"^9.5",
		"typo3/cms-tstemplate":"^9.5",
		"typo3/cms-lowlevel":"^9.5",
		"typo3/cms-rsaauth":"^9.5",
		"typo3/cms-adminpanel": "^9.5",
		"typo3/cms-redirects": "^9.5",
		"helhum/typo3-console": "@stable",
		"clickstorm/cs_seo": "@stable",
		"dirkpersky/typo3-dp_cookieconsent": "@stable",
		"dirkpersky/typo3-rte-ckeditor-fontawesome": "@stable",
		"gridelementsteam/gridelements": "@stable",
		"sgalinski/scriptmerger": "dev-feature_UpgradeTo9LTS",
		"lochmueller/sourceopt": "@dev",
		"lochmueller/staticfilecache": "@stable",
		"georgringer/news": "@stable",
		"in2code/powermail":"@stable",
		"dirkpersky/typo3-composer" : "@stable"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "public"
		}
	},
	"scripts": {
		"post-update-cmd": [
			"@webkon-symlink-version"
		],
		"post-install-cmd": [
			"@webkon-symlink-version"
		],
		"webkon-symlink-version": [
			"DirkPersky\\Typo3Composer\\Classes\\Installer::setVersion"
		],
		"typo3-cms-scripts": [
			"vendor/bin/typo3cms install:fixfolderstructure",
			"vendor/bin/typo3cms install:generatepackagestates"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		]
	}
}
